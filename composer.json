{
    "name": "pqstudio/rest-upload-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle for file uploads in Rest API",
    "keywords": ["rest", "bundle", "file", "upload", "symfony2"],
    "homepage": "https://github.com/PQstudio/RestUploadBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Grzegorz Graczyk",
            "email": "gregory90@gmail.com"
        }
    ],

    "require": {
        "php": ">=5.5.1",
        "friendsofsymfony/rest-bundle": "*",
        "symfony/framework-bundle": ">=2.3,<3.0",
        "knplabs/knp-gaufrette-bundle": "dev-master",
        "pqstudio/rest-utility-bundle": "dev-master"
    },

    "require-dev": {
        "symfony/finder": "2.3.*"
    },

    "autoload": {
        "psr-0": { "PQstudio\\RestUploadBundle": "" }
    },

    "minimum-stability": "dev",
    "target-dir": "PQstudio/RestUploadBundle"
}
